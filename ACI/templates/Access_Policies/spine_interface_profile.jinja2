/*
API Information:
 - Class: "infraSpAccPortP"
 - Distinguished Name: "uni/infra/spaccportprof-{{Name}}"
GUI Location:
 - Fabric > Access Policies > Interfaces > Spine Interfaces > Profiles > {{Name}}
*/
resource "aci_spine_interface_profile" "Interface_Profile_{{Name}}" {
{%- if Tags %}
    annotation  = "{{Tags}}"{% endif %}
{%- if Description %}
    description = "{{Description}}"{% endif %}
    name        = "{{Name}}"
{%- if Alias %}
    name_alias  = "{{Alias}}"{% endif %}
}
