#------------------------------------------------
# Create a BGP Peer Connectivity Profile
#------------------------------------------------

/*
API Information:
 - Class: "bgpPeerPfxPol"
 - Distinguished Name: "uni/tn-common/bgpPfxP-default"
GUI Location:
 - Tenants > common > Networking > Policies > Protocol > BGP >  BGP Peer Prefix > default
*/
resource "aci_bgp_peer_prefix" "common_bgp_pfx_policy_default" {
    depends_on      = [
        aci_tenant.common
    ]
    tenant_dn       = aci_tenant.common.id
    action          = "log"
    description     = "Default Prefix Policy"
    name            = "default"
    max_pfx         = "20000"
    restart_time    = "65535"
    thresh          = "75"
}

